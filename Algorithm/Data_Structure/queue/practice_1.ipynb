   "source": [
    "from collections import deque\n",
    "\n",
    "def solution(n, k):\n",
    "    queue = deque([i+1 for i in range(n)])\n",
    "\n",
    "    while len(queue) > 1:\n",
    "        for _ in range(k-1) : \n",
    "            queue.append(queue.popleft())\n",
    "        queue.popleft()\n",
    "\n",
    "    return queue[0]"
   ]
